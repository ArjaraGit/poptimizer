{{define "search"}}
    <ul id="tickers-search" hx-swap-oob="innerHTML:#tickers-search">
        {{range .}}
            <li>
                <a title="Add" hx-post="tickers/add/{{ . }}">
                    {{.}}
                    <svg>
                        <line x1=0% y1=65% x2=60% y2=65% />
                        <line x1=30% y1=95% x2=30% y2=35% />
                    </svg>
                </a>
            </li>
        {{end}}
    </ul>
{{end}}